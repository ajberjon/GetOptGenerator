{% extends "DafuerGetOptGeneratorBundle::layout.html.twig" %}


{% form_theme form _self %}

{% block form_row %}
{% spaceless %}
        <td>
            {{ form_errors(form) }}
            {{ form_widget(form) }}
        </td>
{% endspaceless %}
{% endblock form_row %}

{% block bundle_content %}
 
<form action="{{ path('DafuerGetOptGeneratorBundle_project_create') }}" method="post" {{ form_enctype(edit_form) }}>
{% include 'DafuerGetOptGeneratorBundle:Project:_form.html.twig' with {'edit_form': form} %}  
        
    <p>
        <button class="btn btn-success" type="submit">Create</button>
    </p>
</form>

